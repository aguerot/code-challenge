{
  "root": "libs/consent-management-mongo",
  "sourceRoot": "libs/consent-management-mongo/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/consent-management-mongo/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/consent-management-mongo"],
      "options": {
        "jestConfig": "libs/consent-management-mongo/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["database"]
}
